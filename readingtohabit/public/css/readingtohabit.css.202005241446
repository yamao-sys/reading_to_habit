}

@media screen and (max-width:576px) {
    .register_user_content,.login_content {
        width: 270px;
        margin-top: 3rem;
        margin-bottom: 3rem;
    }
    .register_user_input,.register_user_check,.register_user_finish {
        display: -ms-flexbox;
        display: flex;
        width: 90%;
