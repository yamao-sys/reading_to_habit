input[type="checkbox"] {
    display: none;
}

input#auto_login[type="checkbox"]+label {
    position: relative;
    padding-left: 1rem;
}

input#auto_login[type="checkbox"]+label::before {
    content: '';
    display: block;
    width: 24px;
    height: 24px;
    position : absolute;
    top: 50%;
    left: 0;
    transform: translateY(-50%);
    border: 1px solid blue;
    border-radius: 3px;
    cursor: pointer;
}

input#auto_login[type="checkbox"]:checked+label::before {
    backgrond: cornflowerblue;
}

input#auto_login[type="checkbox"]:checked+label::after {
    content: '';
    display: block;
    width: 10px;
    height: 6px;
    box-sizing: border-box;
    border-bottom: 2px solid #fff;
    border-left: 2px solid #fff;
    position: absolute;
    top: 40%;
    left: 5px;
    -webkit-transform: rotate(-45deg);
    transform: rotate(-45deg) translateY(-50%);
}

