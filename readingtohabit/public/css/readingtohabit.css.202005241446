body,#wrapper {
    display: -ms-flexbox;
    display: flex;
    -ms-flex-direction: column;
    flex-direction: column;
    min-height: 100vh;
    color: #495057;
}

.header_before_login_service {
    display: -ms-flexbox;
    display: flex;
    width: 100%;
    height: 55px;
    -ms-flex-pack: center;
    justify-content: center;
    -ms-flex-align: center;
    align-items: center;
    background-color: #363a40;
}

.service_area {
    width: 90%;
}

.service {
    color: #fff;
}

.modal_area {
    display: -ms-flexbox;
    display: flex;
    position: fixed;
    width: -webkit-calc(100% - 1rem);
    width: calc(100% - 1rem);
    margin-top: 0.5vh;
    z-index: 1000;
    -ms-flex-pack: center;
    justify-content: center;
    -ms-flex-align: center;
    align-items: center;
}

.header_before_login_body {
    display: -ms-flexbox;
    display: flex;
    width: 100%;
    height: 55px;
    -ms-flex-pack: center;
    justify-content: center;
    -ms-flex-align: center;
    align-items: center;
}

.header_body_area {
    display: -ms-flexbox;
    display: flex;
    width: 90%;
}

.header_before_login_logo {
    margin-right: auto;
    margin-bottom: 0;
}

.header_navi {
    display: -ms-flexbox;
    display: flex;
    -ms-flex-align: center;
    align-items: center;
}

.header_navi_before_login_username {
    margin-right: 1rem;
}

.content_area {
    display: -ms-flexbox;
    display: flex;
    width: 100%;
    -ms-flex-pack: center;
    justify-content: center;
}

.title {
    margin-bottom: 3rem;
}

