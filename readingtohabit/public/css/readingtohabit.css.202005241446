    width: 100px;
    height: calc(1.5rem + 1.5rem + 2px);
    font-weight: 700;
    color: #fff;
    text-align: center;
    vertical-align: middle;
    cursor: pointer;
    -webkit-user-select: none;
    -moz-user-select: none;
    -ms-user-select: none;
    user-select: none;
    background-color: #00a0e9;
    border: 1px solid #00a0e9;
    padding: 0.375rem 0.75rem;
    font-size: 1rem;
    line-height: 1.5;
    border-radius: 0.25rem;
}


.search_result_area {
    border-top: 1px solid #495057;
    padding-top: 1rem;
    padding-bottom: 1rem;
}

.link_dark {
    color: #495057;
    text-decoration: none;
}

.link_white {
    color: #fff;
    text-decoration: none;
}

.search_result_content {
    display: -ms-flexbox;
    display: flex;
}

.thumbnail {
    display: -ms-flexbox;
    display: flex;
    margin-right: 1rem;
    -ms-flex-pack: center;
    justify-content: center;
    -ms-flex-align: center;
    align-items: center;
}

.thumbnail_size {
    width: 100px;
    height: auto;
}

.bookname {
    font-weight: 700;
    font-size: 1.5rem;
    margin-bottom: 1rem;
}

.book_info {
    //
}

#show_article {
    width: 100%;
}

.add_article_book_detail,.show_article_book_detail,.edit_article_book_detail {
    margin-bottom: 2rem;
}

.add_article_bookimg_content,.show_article_bookimg_content,.edit_article_bookimg_content {
    display: -ms-flexbox;
    display: flex;
    -ms-flex-pack: center;
    justify-content: center;
    -ms-flex-align: center;
    align-items: center;
}

.add_article_bookimg,.show_article_bookimg,.edit_article_bookimg {
    width: 200px;
    height: auto;
}

.search_result {
    display: -ms-flexbox;
    display: flex;
    width: 100%;
    height: calc(1.5rem + 0.75rem + 2px);
    -ms-flex-align: center;
    align-items: center;
    z-index: 1000;
    border: 1px solid #363a40;
}

.search_result {
    display: -ms-flexbox;
    display: flex;
    width: 100%;
    height: calc(1.5rem + 0.75rem + 2px);
    -ms-flex-align: center;
    align-items: center;
    z-index: 1000;
    border: 1px solid #363a40;
    background-color: #f7f7f7;
}

.search_result_imageLinks,.search_result_authors,.search_result_categories,.add_article_mail_timing_none,.add_article_mail_timing_select_none,#default_mail_timing_select,.add_article_mail_timing_by_day_none,.add_article_mail_timing_by_week_none,.add_article_mail_timing_by_month_none,#default_mail_flag,#article_id,#favorite_flag {
    display: none;
}

